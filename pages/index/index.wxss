/**index.wxss**/
.banner {
	width: 100%;
	height: 300rpx;
	overflow: hidden;
}
.back-top {
	position: fixed;
	bottom: 180rpx;
	right: 30rpx;
	width: 66rpx;
	height: 66rpx;
	background-color: #eee;
	border-radius: 50rpx;
	text-align: center;
	line-height: 66rpx;
	transition: 0.3s;
	z-index: 9;
}
.back-top image {
	width: 60rpx;
	height: 60rpx;
}